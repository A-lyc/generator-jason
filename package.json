{
  "name": "generator-jason",
  "version": "1.1.0",
  "description": "基于 webpack 的前端多页面脚手架",
  "homepage": "https://github.com/q-jason/generator-jason",
  "author": {
    "name": "q-jason",
    "email": "596908546@qq.com",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "webpack",
    "javascript",
    "yeoman",
    "html",
    "css",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "jest": "^22.0.6",
    "lint-staged": "^6.1.1",
    "nsp": "^2.8.0",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/q-jason/generator-jason.git",
  "license": "ISC"
}
