<!doctype html>
<html lang="zh-CN">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>admin</title>
</head>
<body class="page-admin">
<main class="root">
  <div class="root-group">
    <h3 class="mb-4">
      页面
    </h3>
    <div class="list-group">
      <a
        class="list-group-item"
        v-for="htmlName in htmlNameArr"
        :key="htmlName"
        :href="'./' + htmlName"
        v-html="htmlName">
      </a>
    </div>
  </div>
  <div class="root-group">
    <h3 class="mb-4">
      组件
    </h3>
    <div class="component">
      <div
        class="component-group"
        v-for="comp in compArr"
        :key="comp.dirname">
        <h4
          class="component-group-title"
          v-html="comp.dirname">
        </h4>
        <div
          class="component-group-body"
          v-html="comp.render()">
        </div>
      </div>
    </div>
  </div>
</main>
</body>
</html>
